  "scripts": {
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "zelfrontdev": "vue-cli-service serve --port 16126",
    "zelfrontprod": "nodemon ZelFront/server.js",
    "zelfrontstart": "npm run zelfrontprod",
    "zelfrontbuild": "vue-cli-service build",
    "zelfrontbuildwatch": "vue-cli-service build --watch",
    "zelbackstart": "nodemon ZelBack/server.js",
    "zelbackdev": "nodemon ZelBack/server.js",
    "flux": "nodemon app.js",
    "start": "npm install && node init.js && (npm run zelfrontbuildwatch & npm run flux)",
    "dev": "node init.js && (npm run zelbackdev & npm run zelfrontdev)",
    "updateflux": "git pull && npm install",
    "hardupdateflux": "rm -rf node_modules && rm -f package-lock.json && git reset --hard && git pull && npm run start"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bitcoin": "^3.0.1",
